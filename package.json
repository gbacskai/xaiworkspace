{
  "name": "xaiworkspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "build:mobile": "ng build --configuration production && npx cap sync",
    "cap:sync": "npx cap sync",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "gplay:auth": "node scripts/google-play-publish.mjs auth",
    "gplay:build": "node scripts/google-play-publish.mjs build",
    "gplay:publish": "node scripts/google-play-publish.mjs publish",
    "gplay:upload": "node scripts/google-play-publish.mjs upload",
    "gplay:metadata": "node scripts/google-play-metadata.mjs",
    "devicefarm": "node scripts/device-farm-test.mjs",
    "devicefarm:status": "node scripts/device-farm-test.mjs --list-runs",
    "devicefarm:browser": "node scripts/device-farm-browser.mjs",
    "devicefarm:chrome": "node scripts/device-farm-browser.mjs --browser chrome",
    "devicefarm:firefox": "node scripts/device-farm-browser.mjs --browser firefox"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.9.0",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@capacitor/android": "^8.1.0",
    "@capacitor/app": "^8.0.1",
    "@capacitor/browser": "^8.0.1",
    "@capacitor/core": "^8.1.0",
    "@capacitor/haptics": "^8.0.1",
    "@capacitor/ios": "^8.1.0",
    "@capacitor/keyboard": "^8.0.1",
    "@capacitor/preferences": "^8.0.1",
    "@capacitor/push-notifications": "^8.0.1",
    "@capacitor/splash-screen": "^8.0.1",
    "@capacitor/status-bar": "^8.0.1",
    "@capgo/capacitor-native-biometric": "^8.4.2",
    "chart.js": "^4.5.1",
    "chartjs-plugin-annotation": "^3.1.0",
    "marked": "^17.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^8.1.0",
    "@playwright/test": "^1.58.2",
    "selenium-webdriver": "^4.41.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  }
}
